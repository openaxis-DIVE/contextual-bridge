{
  "manifest_version": 3,
  "name": "Kagi Saver",
  "version": "1.0",
  "permissions": ["storage", "clipboardRead", "offscreen"],
  "content_scripts": [{
    "matches": ["https://kagi.com/*"],
    "js": ["content.js"],
    "all_frames": true,
    "run_at": "document_end"
  }],
  "background": {
    "service_worker": "background.js"
  }
}
